            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/t9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Post                        : "
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />

